{
  "name": "qapp",
  "version": "0.1.0",
  "private": true,
  "description": "QApp - Crowdsourced question paper sharing platform for students",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter qapp-frontend dev",
    "build": "pnpm --filter frontend build",
    "build:packages": "pnpm --filter \"@qapp/*\" build",
    "db:generate": "pnpm --filter database generate",
    "db:migrate": "pnpm --filter database migrate",
    "db:studio": "pnpm --filter database studio",
    "db:seed": "pnpm --filter database seed",
    "test:supabase": "tsx --env-file=./.env scripts/test-supabase.ts",
    "test:r2": "tsx --env-file=./.env scripts/test-r2-connection.ts",
    "lint": "pnpm --filter frontend lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.712.0",
    "@aws-sdk/s3-request-presigner": "^3.712.0",
    "@types/node": "^22.10.5",
    "postgres": "^3.4.5",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0"
  }
}
